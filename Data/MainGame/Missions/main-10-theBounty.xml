		</dialog>
		<dialog>
			<dialogCharacterType attr1="non_player"/>
			<characterID attr1="taz"/>
			<content attr1="Oh no no no!"/>
			<content attr1="NO NO NO NO!"/>
			<content attr1="OUR STUB? OUR STUB!"/>
		</dialog>
		<dialog>
			<dialogCharacterType attr1="non_player"/>
			<characterID attr1="taz"/>
			<content attr1="Don't you see they want revenge?"/>
		</dialog>
		<dialog>
			<dialogCharacterType attr1="non_player"/>
			<characterID attr1="stubMaleGeneric02"/>
			<nameOverride attr1="ELEPHANT FACE"/>
			<content attr1="Enough wasting time."/>
			<content attr1="Let's proceed."/>
			<content attr1="Go ahead Hammond."/>
		</dialog>
		<dialog>
			<dialogCharacterType attr1="non_player"/>
			<characterID attr1="taz"/>
			<content attr1="What? What is he talking about?"/>
			<content attr1="Hammond? HAMMOND?"/>
			<content attr1="What is that?"/>
			<content attr1="No! NO NOOO!"/>
		</dialog>
		<dialog>
			<dialogCharacterType attr1="non_player"/>
			<characterID attr1="hammond"/>
			<content attr1="Uhh... Hold still... Buddy..."/>
		</dialog>
		<dialog>
			<dialogCharacterType attr1="non_player"/>
			<characterID attr1="taz"/>
			<content attr1="I knew it! I KNEW IT!!!"/>
			<content attr1="I'm going to haunt you!"/>
			<content attr1="AAAAAAHHHH!!! AAAHHHH!!!"/>
		</dialog>
		<dialog>
			<dialogCharacterType attr1="non_player"/>
			<characterID attr1="hammond"/>
			<content attr1="Almost ready..."/>
