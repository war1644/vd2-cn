			<content attr1="You idiot!"/>
			<content attr1="That ship IS THE PRIDE!"/>
		</dialog>
		<dialog>
			<dialogCharacterType attr1="non_player"/>
			<characterID attr1="taz"/>
			<content attr1="You know... I was almost proud of you when you dumped me on that rock."/>
